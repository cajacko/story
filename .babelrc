{
  "presets": ["es2015", "stage-2"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "src": "./src",
          "tmp": "./tmp",
          "root": "./",
          "scripts": "./scripts"
        },
        "extensions": [".js"]
      }
    ]
  ]
}
